{
  "name": "sudosos-back-end",
  "version": "0.1.0",
  "description": "Back-end API and database management for SudoSOS.",
  "main": "out/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "ts-node-dev src/index.ts",
    "serve": "node out/src/index.js",
    "swagger": "ts-node-dev src/swagger.ts",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "coverage": "nyc npm run test",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint-fix": "eslint 'src/**/*.ts' 'test/**/*.ts' --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dinero.js": "^1.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.17",
    "express-swaggerize-ui": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "swagger-model-validator": "^3.0.20",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/dinero.js": "^1.6.5",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-swag": "^0.1.9",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
