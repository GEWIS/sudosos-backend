{
  "name": "sudosos-back-end",
  "version": "0.1.0",
  "description": "Back-end API and database management for SudoSOS.",
  "main": "out/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "ts-node-dev src/index.ts",
    "serve": "node out/src/index.js",
    "swagger": "ts-node-dev src/swagger.ts",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "coverage": "nyc npm run test",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint-fix": "eslint 'src/**/*.ts' 'test/**/*.ts' --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dinero.js": "^1.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.17",
    "express-swaggerize-ui": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "swagger-model-validator": "^3.0.19",
    "typeorm": "^0.2.22",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/dinero.js": "^1.6.2",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.8",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-swag": "^0.1.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.2.0",
    "nyc": "^15.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.2"
  }
}