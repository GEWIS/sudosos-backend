{
  "entryPoints": ["src/**/*.ts"],
  "name": "SudoSOS Back-end API",
  "docsRoot": "./docs",
  "out": "docs/out",
  "includeVersion": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-merge-modules", "typedoc-vitepress-theme", "typedoc-plugin-markdown"],
  "mergeModulesRenameDefaults": true,
  "mergeModulesMergeMode": "module",
  "githubPages": false,
  "readme": "none",
  "entryFileName": "index.md",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "disableSources": true
}
